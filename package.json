{
  "name": "reviewr",
  "version": "1.0.0",
  "description": "Unified CLI for scraping and analyzing hotel/property reviews from Booking.com and Airbnb",
  "main": "dist/cli.js",
  "bin": {
    "reviewr": "./dist/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/booking/scraper.ts",
    "dev:cli": "tsx src/cli.ts",
    "start": "tsx src/booking/scraper.ts",
    "airbnb": "tsx src/airbnb/scraper.ts",
    "analytics": "tsx src/booking/analytics.ts",
    "analytics:12m": "tsx src/booking/analytics.ts --12m",
    "analytics:airbnb": "tsx src/airbnb/analytics.ts",
    "analytics:airbnb:12m": "tsx src/airbnb/analytics.ts --12m",
    "transform": "tsx src/booking/transform-to-csv.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.0.13",
    "@types/papaparse": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "commander": "^14.0.3",
    "dotenv": "^17.2.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "turf": "^3.0.14"
  }
}
